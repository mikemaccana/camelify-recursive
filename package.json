	"description": "Recursively convert an API response to camelCase",
	"main": "index.js",
	"scripts": {
		"test": "npx mocha --ui tdd"
	},
	"keywords": [
		"camelCase",
		"underscore",
		"API",
		"response",
		"REST",
		"snakecase",
		"case"
	],
	"author": "Mike MacCana <mike.maccana@gmail.com>",
	"license": "MIT",
	"dependencies": {},
	"devDependencies": {
		"assert": "^2.0.0",
		"mocha": "^10.2.0"
	},
	"repository": {
		"type" : "git",
		"url" : "https://github.com/mikemaccana/camelify-recursive"
	},
	"prettier": {
		"semi": false,
		"singleQuote": true,
		"tabWidth": 2,
